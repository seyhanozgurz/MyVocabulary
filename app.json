{
  "expo": {
    "name": "my-vocabulary",
    "slug": "my-vocabulary",
    "version": "1.0.0",
    "orientation": "default",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.seyhanozgurz.myvocabulary"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "41d11aef-f64f-4115-b59b-24b44e4aece8"
      }
    }
  }
}
